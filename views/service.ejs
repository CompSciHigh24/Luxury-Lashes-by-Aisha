  <!-- Define a for loop that goes through every item and close it after the card -->




  <% for(let i = 0; i < service.length; i++) { %>
    <div class="card mb-3" style="max-width: 540px;">
      <div class="row g-0">
        <div class="col-md-4">
        <img src="<%= service[i].image %>" class="card-img-top" alt="...">
        </div>
        <div class="col-md-8">
        <div class="card-body">
          <h5 class="card-title"><%= service[i].style%></h5>
          <div class="col-12">
          <p class="card-text">$<%= service[i].price %></p>
            <a href="https://56b88286-da0b-4f27-90ce-ecfb88529ff7-00-2o611wcc55zei.kirk.replit.dev:3003/booking"<button class="btn btn-primary" type="submit">Book</button></a>
          <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
        </div>
      </div>
        </div>
      </div>
      </div>

  <% } %>




  <script src="index.js"></script>
